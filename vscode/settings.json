{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.pyc": true,
    "**/*.idx": true,
    "**/*.fls": true,
    "**/*.dvi": true,
    "**/*.aux": true,
    "**/*.blg": true,
    "**/*.bbl": true,
    "**/*.gz": true,
    "**/*.toc": true
  },
  "workbench.statusBar.feedback.visible": false,
  "workbench.activityBar.visible": true,
  "editor.minimap.enabled": false,
  "explorer.confirmDelete": false,
  "files.autoSave": "afterDelay",
  "C_Cpp.clang_format_path": "/usr/local/bin/clang-format",
  "C_Cpp.clang_format_fallbackStyle": "none",
  "C_Cpp.clang_format_style": "file",
  "code-runner.runInTerminal": true,
  "C_Cpp.default.cppStandard": "c++11",
  "C_Cpp.default.cStandard": "c11",
  "code-runner.executorMap": {
    "cpp": "g++ $fileName -o $fileNameWithoutExt && ./$fileNameWithoutExt",
    "python": "python3"
  },
  "code-runner.clearPreviousOutput": true,
  "[latex]": {
    "editor.wordWrap": "on",
    "editor.wordWrapColumn": 78,
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": false
    }
  },
  "[bibtex]": {
    "editor.wordWrap": "on",
    "editor.wordWrapColumn": 78
  },
  "breadcrumbs.enabled": true,
  "editor.formatOnType": true,
  "[markdown]": {
    "editor.formatOnSave": true,
    "editor.wordWrap": "on",
    "editor.renderWhitespace": "all",
    "editor.acceptSuggestionOnEnter": "off"
  },
  "editor.fontFamily": "Fira Code, Bitstream Vera Sans Mono, Consolas, Hack, Menlo",
  "editor.fontLigatures": false,
  "editor.lineHeight": 20,
  "editor.formatOnPaste": true,
  "editor.fontSize": 14,
  "window.zoomLevel": 0,
  "files.maxMemoryForLargeFilesMB": 8192,
  "explorer.confirmDragAndDrop": false,
  "workbench.colorTheme": "NetBeans Light Theme",
  "[python]": {
    "editor.formatOnPaste": false
  },
  "[latex]": {
    "editor.formatOnSave": false
  },
  "python.formatting.provider": "yapf",
  "python.jediEnabled": false,
  "latex-workshop.latex.clean.subfolder.enabled": true,
  "latex-workshop.message.badbox.show": false,
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.chktex.enabled": false,
  "latex-workshop.message.update.show": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "code-runner.fileDirectoryAsCwd": true,
  "python.linting.flake8CategorySeverity.W": "Error",
  "html.format.wrapLineLength": 72,
  "terminal.integrated.inheritEnv": false,
  "git.ignoreLegacyWarning": true,
  "terminal.integrated.shell.osx": "/bin/zsh",
  "terminal.integrated.env.osx": {
    "GOPATH": "${workspaceRoot}"
  },
  "sync.gist": "822c5dd94a75b8873c7f46b7a6d73436",
  "go.formatTool": "goimports",
  "go.lintFlags": [
    "-config=${workspaceFolder}/config.toml"
  ],
  "go.lintTool": "revive",
  "go.UseLanguageServer": true,
  "go.docTool": "gogetdoc",
  "files.autoSaveDelay": 30000,
  "go.useLanguageServer": true,
  "go.testFlags": [
    "-v"
  ],
  "editor.formatOnSave": true,
  "markdown.extension.preview.autoShowPreviewToSide": true,
  "go.testEnvFile": "",
  "sync.removeExtensions": false,
  "sync.autoDownload": true,
  "python.languageServer": "Microsoft",
  "go.addTags": {},
  "[go]": {
    "editor.formatOnSave": true,
  },
  "files.associations": {
    "*.go": "go"
  },
  "latex-workshop.latex.autoBuild.interval": 3000,
  "rewrap.reformat": true,
  "rewrap.autoWrap.enabled": true,
  "rewrap.doubleSentenceSpacing": true,
  "rewrap.wrappingColumn": 78,
  "latex-workshop.latex.autoClean.run": "onBuilt",
  // Define which recipe is used by `latex-workshop.build`. It also applies to auto build. Recipes are refered to by their names as defined in `latex-workshop.latex.recipes`. Note there are two particular values: 
  // - `first` means to use the first recipe in `latex-workshop.latex.recipes`;
  // - `lastUsed` means to use the last run recipe.
  "latex-workshop.latex.recipe.default": "lastUsed",
  "hediet.vscode-drawio.local-storage": "",
  "printcode.disableTelemetry": true,
  "printcode.fontSize": 10,
  "printcode.paperSize": "letter",
  "printcode.tabSize": 4,
  "editor.formatOnSaveMode": "modifications",
  "jupyter.alwaysTrustNotebooks": true,
  "workbench.editorAssociations": [
    {
      "viewType": "jupyter.notebook.ipynb",
      "filenamePattern": "*.ipynb"
    }
  ],
  "extensions.ignoreRecommendations": true,
  "jupyter.askForKernelRestart": false
